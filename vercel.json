{
    "version": 2,
    "builds": [
      {
        "src": "./**",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",  
        "dest": "./wsgi.py" 
      }
    ]
  }
  